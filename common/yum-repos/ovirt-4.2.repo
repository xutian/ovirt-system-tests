[main]
reposdir = /etc/reposync.repos.d
max_connections = 10
ip_resolve = 4
gpgcheck = 0
plugins = 1
proxy=http://www-proxy.us.oracle.com:80
no_proxy=ca-artifacts.us.oracle.com

[ovirt-4.2]
name = oVirt 4.2
baseurl = http://ca-artifacts.us.oracle.com/auto-build/ovirt/4.2/x86_64/ 
enabled = 1
gpgcheck = 0

[glusterfs-3.12-el7]
name = GlusterFS repo
baseurl = https://yum.oracle.com/repo/OracleLinux/OL7/developer_gluster312/x86_64/ 
enabled = 1
includepkgs = 
	gdeploy
	glusterfs
	glusterfs-api
	glusterfs-cli
	glusterfs-client-xlators
	glusterfs-fuse
	glusterfs-libs
	glusterfs-rdma

